/dts-v1/;

#include "rp-a311d.dtsi"

/ {
	model = "Vaxpl QX1";
	amlogic-dt-id = "g12b_qx1";
	compatible = "vaxpl,qx1", "amlogic,g12b";

	stm706 {
		status = "okay";
		compatible = "stm706";
		reset_gpio = <&gpio GPIOZ_3 GPIO_ACTIVE_HIGH>;
		wdt_gpio = <&gpio GPIOZ_2 GPIO_ACTIVE_HIGH>;
	};
};

&i2c_AO {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&ao_i2c_master_pins1>;
	clock-frequency = <100000>;

	hym8563: hym8563@51 {
		compatible = "haoyu,hym8563";
		reg = <0x51>;
		#clock-cells = <0>;
	};
};

&uart_A { /* Bluetooth */
	status = "okay";
};

&uart_C { /* RS-485 */
	status = "okay";
};

&uart_AO_B { /* RS-232 */
	status = "okay";
};

