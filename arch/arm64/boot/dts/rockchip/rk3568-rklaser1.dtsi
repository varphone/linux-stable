};

&i2c2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&i2c2m1_xfer>;

	imx385: imx385@1a {
		status = "okay";
		compatible = "sony,imx385";
		reg = <0x1a>;
		clocks = <&cru CLK_CIF_OUT>;
		clock-names = "xvclk";
		power-domains = <&power RK3568_PD_VI>;
		pinctrl-names = "default";
		pinctrl-0 = <&cif_clk>;
		reset-gpios = <&gpio3 RK_PD4 GPIO_ACTIVE_LOW>;
		/*pwdn-gpios = <&gpio4 RK_PB4 GPIO_ACTIVE_HIGH>;*/
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "FV-IMX385";
		rockchip,camera-module-lens-name = "FIXED";
		port {
			imx385_out: endpoint {
				remote-endpoint = <&mipi_in_ucam4>;
				data-lanes = <1 2 3 4>;
			};
		};
	};

	sc2210: sc2210@30 {
		status = "okay";
		compatible = "smartsens,sc2210";
		reg = <0x30>;
		clocks = <&cru CLK_CIF_OUT>;
		clock-names = "xvclk";
		power-domains = <&power RK3568_PD_VI>;
		pinctrl-names = "default";
		pinctrl-0 = <&cif_clk>;
		reset-gpios = <&gpio3 RK_PD4 GPIO_ACTIVE_LOW>;
		/*pwdn-gpios = <&gpio4 RK_PB4 GPIO_ACTIVE_HIGH>;*/
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "FV-SC2210";
		rockchip,camera-module-lens-name = "FIXED";
		// NO_HDR:0 HDR_X2:5 HDR_X3:6
		rockchip,camera-hdr-mode = <5>;
		port {
			sc2210_out: endpoint {
				remote-endpoint = <&mipi_in_ucam3>;
				data-lanes = <1 2 3 4>;
			};
		};
	};

	sc130gs: sc130gs@30 {
		status = "okay";
		compatible = "smartsens,sc130gs";
		reg = <0x30>;
		clocks = <&cru CLK_CIF_OUT>;
		clock-names = "xvclk";
		power-domains = <&power RK3568_PD_VI>;
		pinctrl-names = "default";
		pinctrl-0 = <&cif_clk>;
		/*reset-gpios = <&gpio3 RK_PD4 GPIO_ACTIVE_LOW>;*/
		pwdn-gpios = <&gpio3 RK_PD4 GPIO_ACTIVE_HIGH>;
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "FV-SC130GS";
		rockchip,camera-module-lens-name = "FIXED";
		// NO_HDR:0 HDR_X2:5 HDR_X3:6
		rockchip,camera-hdr-mode = <0>;
		port {
			sc130gs_out: endpoint {
				remote-endpoint = <&mipi_in_ucam5>;
				data-lanes = <1 2 3 4>;
			};
		};
	};
};

