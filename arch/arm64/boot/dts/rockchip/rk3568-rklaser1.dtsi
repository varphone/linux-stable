&csi2_dphy0 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;
		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_in_ucam0: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&ucam_out0>;
				data-lanes = <1 2 3 4>;
			};
			mipi_in_ucam1: endpoint@2 {
				reg = <2>;
				remote-endpoint = <&gc8034_out>;
				data-lanes = <1 2 3 4>;
			};
			mipi_in_ucam2: endpoint@3 {
				reg = <3>;
				remote-endpoint = <&ov5695_out>;
				data-lanes = <1 2>;
			};
			mipi_in_ucam3: endpoint@4 {
				reg = <4>;
				remote-endpoint = <&sc2210_out>;
				data-lanes = <1 2 3 4>;
			};
			mipi_in_ucam4: endpoint@5 {
				reg = <5>;
				remote-endpoint = <&imx385_out>;
				data-lanes = <1 2 3 4>;
			};
			mipi_in_ucam5: endpoint@6 {
				reg = <6>;
				remote-endpoint = <&sc130gs_out>;
				data-lanes = <1 2 3 4>;
			};
		};
		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			csidphy_out: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&isp0_in>;
			};
		};
	};
};

